{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#D8DEE9",
          "background": "#434C5E",
          "leading_diamond": "<#5E81AC,transparent>\u256d\u2500</>\ue0b6",
          "trailing_diamond": "<#434C6A,transparent>\ue0b4",
          "template": " {{ if .WSL }}\uf17a WSL{{ end }} "
        },
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#D8DEE9",
          "background": "#5E81AC",
          "leading_diamond": "\u2500\ue0b6",
          "trailing_diamond": "<#5E81AC,transparent>\ue0b0",
          "template": " {{ if .WSL }}{{.Icon}}  {{ end }}",
          "properties": {
            "macos": "\udb80\udc36"
          }
        },
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#D8DEE9",
          "background": "#5E81AC",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "trailing_diamond": "<#5E81AC,transparent>\ue0b0",
          "template": " {{ if not .WSL }}{{.Icon}} {{ end }}",
          "properties": {
            "macos": "\udb80\udc36"
          }
        },
        {
          "type": "command",
          "style": "diamond",
          "foreground": "#2E3440",
          "background": "#81A1C1",
          "trailing_diamond": "<#81A1C1,transparent>\ue0b0",
          "template": "<transparent,#81A1C1>\ue0b0</> \uf4bc  {{ .Output }} ",
          "properties": {
            "command": "uname -m"
          }
        },
        {
          "type": "shell",
          "style": "diamond",
          "foreground": "#2E3440",
          "background": "#88C0D0",
          "trailing_diamond": "<#88C0D0,transparent>\ue0b0",
          "template": "<transparent,#88C0D0>\ue0b0</> \ue795 {{ .Name }} "
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#2E3440",
          "background": "#8FBCBB",
          "trailing_diamond": "<#8FBCBB,transparent>\ue0b0",
          "template": "<transparent,#8FBCBB>\ue0b0</> {{ if eq .UserName \"jamie\" }}\udb81\ude8c{{ else }}\uf007{{ end }} {{ if .SSHSession }}\udb80\udf18 {{ end }}{{ .UserName }} "
        },
        {
          "type": "root",
          "style": "diamond",
          "foreground": "#D8DEE9",
          "background": "#BF616A",
          "trailing_diamond": "\ue0b0",
          "template": "<transparent>\ue0b0</> \uf292 "
        },
        {
          "type": "path",
          "style": "powerline",
          "foreground": "#2E3440",
          "background": "#EBCB8B",
          "trailing_diamond": "\ue0b0",
          "template": "<transparent>\ue0b0</> \uf07c  {{ .Path }}  <background,transparent>\ue0b0</>",
          "properties": {
            "folder_icon": " \uf07c ",
            "home_icon": "\uf015",
            "style": "folder"
          }
        },
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#2E3440",
          "background": "#A3BE8C",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if eq .Env.MANUFACTURER \"Razer\" }}<transparent>\ue0b0</> {{.Icon}} {{end}}",
          "properties": {
            "linux": "\udb85\udd0e",
            "macos": "\udb85\udd0e",
            "ubuntu": "\udb85\udd0e",
            "windows": "\udb85\udd0e"
          }
        },
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#BF616A",
          "background": "#d8dee9",
          "trailing_diamond": "\ue0b4",
          "template": "{{if ne .Env.MANUFACTURER \"Razer\" }}<transparent>\ue0b0</> {{.Icon}} {{end}}",
          "properties": {
            "linux": "\uf1d0",
            "macos": "\uf1d0",
            "ubuntu": "\uf1d0",
            "windows": "\uf1d0"
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "foreground": "#2E3440",
          "background": "#8FBCBB",
          "leading_diamond": "<#8FBCBB,transparent>\u2500\u2500\ue0b6</>",
          "trailing_diamond": "\ue0b4<#8FBCBB,transparent>\u2500\u2500</>",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} <background,transparent>\ue0b0</><transparent>\ue0b0</> \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} <background,transparent>\ue0b0</><transparent>\ue0b0</>{{ end }}{{ if .Staging.Changed }}<#ECEFF4> \uf046 {{ .Staging.String }}</>{{ end }} ",
          "properties": {
            "branch_icon": "\ue725  ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "github_icon": "\uf113  "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "node",
          "style": "diamond",
          "foreground": "#ECEFF4",
          "background": "#5E81AC",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4<#8FBCBB,transparent>\u2500\u2500</>",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a>\ue5fa</> ",
            "yarn_icon": " <#348cba>\udb80\udd1b</>"
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "foreground": "#ECEFF4",
          "background": "#5E81AC",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4<#8FBCBB,transparent>\u2500\u2500</>",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} "
        },
        {
          "type": "java",
          "style": "diamond",
          "foreground": "#ECEFF4",
          "background": "#5E81AC",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4<#8FBCBB,transparent>\u2500\u2500</>",
          "template": " \ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "foreground": "#ECEFF4",
          "background": "#5E81AC",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4<#8FBCBB,transparent>\u2500\u2500</>",
          "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "go",
          "style": "diamond",
          "foreground": "#ECEFF4",
          "background": "#5E81AC",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4<#8FBCBB,transparent>\u2500\u2500</>",
          "template": " \udb81\udfd3 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "rust",
          "style": "diamond",
          "foreground": "#ECEFF4",
          "background": "#5E81AC",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4<#8FBCBB,transparent>\u2500\u2500</>",
          "template": " \ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "dart",
          "style": "diamond",
          "foreground": "#ECEFF4",
          "background": "#5E81AC",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4<#8FBCBB,transparent>\u2500\u2500</>",
          "template": " \ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "angular",
          "style": "diamond",
          "foreground": "#ECEFF4",
          "background": "#5E81AC",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4<#8FBCBB,transparent>\u2500\u2500</>",
          "template": " \ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "julia",
          "style": "diamond",
          "foreground": "#ECEFF4",
          "background": "#5E81AC",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4<#8FBCBB,transparent>\u2500\u2500</>",
          "template": " <#ca3c34>\ue624</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "ruby",
          "style": "diamond",
          "foreground": "#ECEFF4",
          "background": "#5E81AC",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4<#8FBCBB,transparent>\u2500\u2500</>",
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "azfunc",
          "style": "diamond",
          "foreground": "#ECEFF4",
          "background": "#5E81AC",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4<#8FBCBB,transparent>\u2500\u2500</>",
          "template": " \uf104<#f5bf45>\uf0e7</>\uf105 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "aws",
          "style": "diamond",
          "foreground": "#ECEFF4",
          "background": "#5E81AC",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4<#8FBCBB,transparent>\u2500\u2500</>",
          "template": " \ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}} "
        },
        {
          "type": "kubectl",
          "style": "diamond",
          "foreground": "#ECEFF4",
          "background": "#5E81AC",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " \udb82\udc33 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} "
        },
        {
          "type": "exit",
          "style": "diamond",
          "foreground": "#2E3440",
          "background": "#A3BE8C",
          "background_templates": [
            "{{ if gt .Code 0 }}#BF616A{{ end }}"
          ],
          "leading_diamond": "\ue0b6",
          "template": "{{ if gt .Code 0 }} <#D8DEE9>\uf071  {{ .Meaning }}</>{{ else }}\uf00c{{ end }}<transparent> \ue0b2</>",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "foreground": "#2E3440",
          "background": "#EBCB8B",
          "leading_diamond": "\ue0b2",
          "template": " \udb81\udd1f{{ .FormattedMs }}\u2800<transparent>\ue0b2</>",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          }
        },
        {
          "type": "battery",
          "style": "diamond",
          "invert_powerline": true,
          "foreground": "#2E3440",
          "background": "#8FBCBB",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#D08770{{end}}",
            "{{if eq \"Discharging\" .State.String}}#EBCB8B{{end}}",
            "{{if eq \"Full\" .State.String}}#8FBCBB{{end}}"
          ],
          "leading_diamond": "\ue0b2",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }} %{{ end }}{{ .Error }} <transparent>\ue0b2</>",
          "properties": {
            "charged_icon": "\udb85\udfe6 ",
            "charging_icon": "\uf1e6 ",
            "discharging_icon": "\ue234 "
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "invert_powerline": true,
          "foreground": "#2E3440",
          "background": "#88C0D0",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4 ",
          "template": " \udb80\udcf0 {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "3:04 PM"
          }
        }
      ],
      "filler": "<#8FBCBB,transparent>\u2500</>"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#5E81AC",
          "template": "\u2570\u2500"
        },
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#88C0D0",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#BF616A{{ end }}"
          ],
          "template": "\ue285\udb81\ude01 ",
          "properties": {
            "always_enabled": true
          }
        }
      ],
      "newline": true
    }
  ],
  "transient_prompt": {
    "foreground": "#88C0D0",
    "background": "transparent",
    "template": "\u2500\u2500\ue285\udb81\ude01 "
  }
}
